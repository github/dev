install:
    apt update -y && \
    apt upgrade -y && \
    apt install -y curl  && \
    curl -fsSL https://tailscale.com/install.sh | sh

login:
    tailscale up  --login-server=https://tailscale.rickietangmyspace.top:8080/
    with Preauth Keys:
        tailscale up  --login-server=https://tailscale.rickietangmyspace.top:8080/ --auth-key ac0bc6569d36773ae0ea70249ba49579cb89a692e6a8d090  



log:
    journalctl -u tailscaled --since "5 min ago"    