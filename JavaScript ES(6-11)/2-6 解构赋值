1.
let arr = [1,2,3]
let a = arr[0]
let b = arr[1]
let c = arr[2]
console.log(a,b,c)
输出 1 2 3

2.
let [a,b,c] = [1,2,3]
console.log(a,b,c)
输出 1 2 3

3.
let [a,b,c] = [1,2,[3,4]]
console.log(a,b,c)
输出 1 2 (2) [3, 4]

4.
let [a,b,[c,d]] = [1,2,[3,4]]
console.log(a,b,c,d)
输出 1 2 3 4

5.
let [a,b,[c]] = [1,2,[3,4]]
console.log(a,b,c)
输出 1 2 3

6.
let [a,b,c,d] = [1,2,[3,4]]
console.log(a,b,c,d)
输出 1 2 (2) [3, 4] undefined

7.