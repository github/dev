  public async findTags(ids: string[] = []) {
    console.log('6', ids);
    const tagsDb = await this.guestModel
      .aggregate()
      .unwind('tags')
      .project({
        _id: 0,
        inputValue: '$tags',
      })
     .group({ _id: '$inputValue' });
    const tags = [];
    tagsDb.map((item) => {
      tags.push(item._id);
    });
    return tags;
  }