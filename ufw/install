systemctl disable firewalld
systemctl stop firewalld
apt install -y ufw
echo y | ufw enable
echo y | ufw reset
ufw default deny incoming	
ufw default allow outgoing
ufw allow 443/tcp
ufw allow 41641/udp
ufw allow 3478/udp
ufw allow in on tailscale0 from 10.10.0.0/16
echo y | ufw enable
ufw reload
ufw status

#ufw delete allow SSH
#ufw allow in on tailscale0 to any port 22